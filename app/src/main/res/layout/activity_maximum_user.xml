<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="label15"
            type="String" />
    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent"
    tools:context=".MaximumUserActivity">

    <ImageView
        android:id="@+id/imageView_1_maximum_user"
        android:layout_width="227dp"
        android:layout_height="143dp"
        android:contentDescription="@string/tod"
        android:src="@drawable/profile"
        app:layout_constraintBottom_toTopOf="@+id/textView_1_maximum_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_1_maximum_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@{label15}"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_2_maximum_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_1_maximum_user" />

    <TextView
        android:id="@+id/textView_2_maximum_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textAlignment="center"
        android:textColor="@color/gray"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_3_maximum_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_1_maximum_user" />

    <TextView
        android:id="@+id/textView_3_maximum_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="@string/text13"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView_4_maximum_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_2_maximum_user" />

    <TextView
        android:id="@+id/textView_4_maximum_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/text16"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView_5_maximum_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_3_maximum_user" />

    <TextView
        android:id="@+id/textView_5_maximum_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/text17"
        android:textColor="@color/gray"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_6_maximum_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_4_maximum_user" />

    <TextView
        android:id="@+id/textView_6_maximum_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text18"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_5_maximum_user" />
</androidx.constraintlayout.widget.ConstraintLayout>

</layout>