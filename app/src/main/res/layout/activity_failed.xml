<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="label8"
            type="Integer" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="#FFFFFF"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FailedActivity">

        <TextView
            android:id="@+id/textView_1_verification_failed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/head_fail"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/textView_2_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_2_verification_failed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@{label8}"
            android:textAlignment="center"
            app:layout_constraintBottom_toTopOf="@+id/imageView_1_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_1_verification_failed" />

        <ImageView
            android:id="@+id/imageView_1_verification_failed"
            android:layout_width="80dp"
            android:layout_height="58dp"
            android:layout_marginTop="12dp"
            android:contentDescription="@string/cross"
            app:layout_constraintBottom_toTopOf="@+id/textView_3_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_2_verification_failed"
            app:srcCompat="@drawable/cross" />

        <TextView
            android:id="@+id/textView_3_verification_failed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/list"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView_4_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView_1_verification_failed" />

        <TextView
            android:id="@+id/textView_4_verification_failed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="@string/listitem_one"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView_5_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_3_verification_failed" />

        <TextView
            android:id="@+id/textView_5_verification_failed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/listitem_two"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView_6_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_4_verification_failed" />

        <TextView
            android:id="@+id/textView_6_verification_failed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/listitem_two"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView_7_verification_failed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_5_verification_failed" />

        <TextView
            android:id="@+id/textView_7_verification_failed"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/listitem_three"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_6_verification_failed" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:text="@string/option_one"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="22sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/textView15_editing_home"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_7_verification_failed" />

        <TextView
            android:id="@+id/textView15_editing_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:text="@string/option_two"
            android:textAlignment="center"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="22sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/textView_7_verification_failed" />

    </androidx.constraintlayout.widget.ConstraintLayout></layout>