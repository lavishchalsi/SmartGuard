<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent"
    tools:context=".unregister_user">

    <ImageView
        android:id="@+id/imageView_1_unregister_user"
        android:layout_width="138dp"
        android:layout_height="133dp"
        android:layout_marginStart="136dp"

        android:background="@drawable/profile"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toTopOf="@+id/textView_1_unregister_user"
        app:layout_constraintEnd_toStartOf="@+id/imageView_2_unregister_user"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_1_unregister_user"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginTop="8dp"
        android:text="@string/text10"
        app:layout_constraintBottom_toTopOf="@+id/textView_2_unregister_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_1_unregister_user" />

    <TextView
        android:id="@+id/textView_2_unregister_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/text11"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_1_unregister_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_1_unregister_user" />

    <EditText
        android:id="@+id/editText_1_unregister_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="@string/text11"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textView_3_unregister_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_2_unregister_user" />

    <TextView
        android:id="@+id/textView_3_unregister_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/text12"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_2_unregister_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_1_unregister_user" />

    <EditText
        android:id="@+id/editText_2_unregister_user"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:hint="@string/text12"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/button_1_unregister_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_3_unregister_user" />

    <Button
        android:id="@+id/button_1_unregister_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="@drawable/roundbutton"
        android:text="@string/btn_nine"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_2_unregister_user" />

    <ImageView
        android:id="@+id/imageView_2_unregister_user"
        android:layout_width="68dp"
        android:layout_height="47dp"
        android:contentDescription="@string/todo"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/editText_1_unregister_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_1_unregister_user"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.104" />
</androidx.constraintlayout.widget.ConstraintLayout>